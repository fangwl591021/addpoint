{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/register",
      "methods": ["POST"],
      "dest": "/api/register.js"
    },
    {
      "src": "/api/checkin",
      "methods": ["POST"],
      "dest": "/api/checkin.js"
    }
  ]
}
